{
  "compilerOptions": {
      "module": "ES2020",
      "moduleResolution": "node",
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "outDir": "./scripts",
      "allowJs": true,
      "rootDir": "src",
      "baseUrl": "./src",
  },
  "exclude": [ "node_modules" ],
  "include": [ "src" ]
}